{
  "version": 2,
  "builds": [
    {
      "src": "backend-capture/src/dist/index.ts", 
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/schema.prisma", "prisma/**"]
    },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend-capture/src/dist/index.ts"
    }
  ]
}
